    <select id="selectTempClueinfoList" parameterType="TempClueinfo" resultMap="TempClueinfoResult">
        <include refid="selectTempClueinfoVo"/>
        <where>  
            <if test="id != null  and id != ''"> and ID = #{id}</if>
            <if test="unitname != null  and unitname != ''"> and UnitName like concat('%', #{unitname}, '%')</if>
            <if test="cluecode != null  and cluecode != ''"> and ClueCode = #{cluecode}</if>
            <if test="personname != null  and personname != ''"> and PersonName like concat('%', #{personname}, '%')</if>
            <if test="personunit != null  and personunit != ''"> and PersonUnit = #{personunit}</if>
            <if test="cluesource != null  and cluesource != ''"> and ClueSource = #{cluesource}</if>
            <if test="clueno != null  and clueno != ''"> and ClueNo = #{clueno}</if>
            <if test="createcompanyid != null and createcompanyid != ''"> and createcompanyid in (SELECT id FROM dept WHERE ( id = #{createcompanyid} OR pid = #{createcompanyid} ) AND Category = '单位')</if>
        </where>
    </select>
    
    <select id="selectTempClueinfoById" parameterType="String" resultMap="TempClueinfoResult">
